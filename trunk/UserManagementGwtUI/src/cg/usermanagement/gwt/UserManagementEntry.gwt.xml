<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='usermanagement'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>

  <inherits name='cg.gwt.components.GwtComponents'/>
  <inherits name='cg.usermanagement.usermanagementshared'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='cg.usermanagement.gwt.client.UserManagementPage'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
